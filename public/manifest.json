{
  "name": "DesModder for Desmos",
  "description": "Unofficial plugins for Desmos, bundled together",
  "manifest_version": 3,
  "version": "0.2.0",
  "icons": {
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "https://www.desmos.com/calculator*"
      ],
      "js": [
        "content.js"
      ]
    }
  ],
  "web_accessible_resources": [{
    "resources": ["script.js", "wolfram2desmos.js"],
    "matches": ["https://www.desmos.com/*"]
  }],
  "permissions": [],
  "host_permissions": [
    "https://www.desmos.com/*"
  ],
  "externally_connectable": {
    "matches": ["https://www.desmos.com/*"]
  }
}
