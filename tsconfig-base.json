{
  "compilerOptions": {
    "outDir": "./dist/",
    "sourceMap": true,
    "strictNullChecks": true,
    "module": "es6",
    "moduleResolution": "bundler",
    "allowSyntheticDefaultImports": true,
    "jsx": "react",
    "jsxFactory": "jsx",
    "target": "es2020",
    "allowJs": true,
    "strict": true,
    "baseUrl": ".",
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "skipLibCheck": true
  }
}
